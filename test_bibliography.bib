@inproceedings{SidorovEtal2014,
    title     = {Automatic Recognition of Personality Traits: A Multimodal Approach},
    author    = {Sidorov, Maxim and Ultes, Stefan and Schmitt, Alexander},
    year      = 2014,
    booktitle = {Proceedings of the 2014 Workshop on Mapping Personality Traits Challenge and Workshop},
    location  = {Istanbul, Turkey},
    publisher = {Association for Computing Machinery},
    address   = {New York, NY, USA},
    series    = {MAPTRAITS '14},
    pages     = {11–15},
    doi       = {10.1145/2668024.2668028},
    isbn      = 9781450339568,
    url       = {https://doi.org/10.1145/2668024.2668028},
    abstract  = {A system being capable of recognize personality traits may be utilized in an enormous number of applications. Adding personality-dependency may be useful to build speaker-adaptive models, e.g., to improve Spoken Dialogue Systems (SDSs) or to monitor agents in call-centers. Therefore, the First Audio/Visual Mapping Personality Traits Challenge (MAPTRAITS 2014) focuses on estimating personality traits. In this context, this study presents the results for multimodal recognition of personality traits using support vector machines. As only small portions of the data is used for personality estimation at a time (which are later combined to a final estimate), different segmentation methods (and how to derive a final hypothesis) are analyzed regarding the task as both a regression and a classification problem.},
    numpages  = 5,
    keywords  = {support vector machine, personality traits, feature-based fusion, audio-visual features}
}

