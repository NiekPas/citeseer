@article{vanZolingenEtal2012,
    title    = {Cross-cultural training for Dutch expatriates going to India},
    author   = {van Zolingen, Simone J. and Essers, Caroline and Vermeer, Lisan},
    year     = 2012,
    journal  = {European Journal of International Management},
    volume   = 6,
    number   = 1,
    pages    = {10--28},
